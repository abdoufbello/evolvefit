# EvolveFit Environment Configuration

# n8n Webhook URLs
N8N_LLM_WEBHOOK_URL=https://your-n8n-instance.com/webhook/llm
N8N_NOTION_WEBHOOK_URL=https://your-n8n-instance.com/webhook/notion

# Application Settings
APP_NAME=EvolveFit
APP_VERSION=1.0.0
APP_DOMAIN=evolvefit.leplustudio.top

# Traefik Configuration
TRAEFIK_NETWORK=traefik
TRAEFIK_CERT_RESOLVER=leresolver

# LLM Configuration
LLM_MODEL=gpt-4
LLM_MAX_TOKENS=2000
LLM_TEMPERATURE=0.7

# Notion Configuration
NOTION_DATABASE_ID=your-notion-database-id
NOTION_API_VERSION=2022-06-28

# Sync Settings
SYNC_INTERVAL_MINUTES=30
SYNC_RETRY_ATTEMPTS=3
SYNC_TIMEOUT_SECONDS=30

# Security
CSP_ENABLED=true
SECURE_HEADERS=true

# Development
DEBUG_MODE=false
LOG_LEVEL=info